#include <iostream>
#include<cmath>
#include<algorithm>
#include <vector>
#define el <<"\n"
#define cel cout<<"\n"
#define ll long long
const ll maxn = 1e5 + 10;
int a[maxn];
using namespace std;
int main()
{
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++)
    {
        cin>>arr[i];
        a[arr[i]]++;
    }
    int x = sizeof(a) / sizeof(a[0]);
    sort(a,a+x);
    cout<<a[x-1];

}
